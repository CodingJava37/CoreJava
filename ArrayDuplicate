public class ArrayDuplicate{  
    
    public static int duplicateNumber(int arr[]) {
       int i,j,count;
        for(i=0;i<arr.length;i++){
            count=0;
            for(j=0;j<arr.length;j++){
                if(arr[i]==arr[j])
                    count++;
            }
            if(count > 1)
            {
                return arr[i];
            }
        }
        return 0;
    }
    
}
