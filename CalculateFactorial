 public static void main(String[] args) { //caller funtion
        int factorialRes = calculateFactorial(5);
        System.out.println("Factorial for the given number is : " + factorialRes);
    }

    private static int calculateFactorial(int num){
        int fact =1;
        for(int i =1; i <=num ;i++){
            fact = fact*i;
        }
        return fact;
    }
